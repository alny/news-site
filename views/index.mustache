  <div class="container inner">
    <div class="row">
      <div class="col-sm-8 content">
        <div class="blog-posts grid-view col2">
          <div class="isotope row">

            {{#article}}
            <div class="col-md-6 col-sm-12 post-grid">
              <div class="post">
                <figure class="main"><a href="/post?id={{_id}}">
                  <div class="text-overlay">
                    <div class="info"><span></span></div>
                  </div>
                  <img src="{{image}}" alt="" /></a></figure>
                <div class="meta"><span class="date">{{updated}}</span><span class="comments"><a href="#"><i class="icon-chat-1"></i></a></span></div>
              <h2 class="post-title"><a href="/post?id={{_id}}">{{title}}</a></h2>
                <p>{{description}}</p>
              </div>
              <!-- /.post -->
            </div>
            {{/article}}


          </div>
          <!-- /.isotope -->
		</div>
        <!-- /.blog-posts -->

        <div class="pagination">
          <ul>
            <li><a href="#" class="btn">Prev</a></li>
